{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": [
        "@babel/plugin-syntax-import-assertions"
      ]
    }
  },
  "rules": {
    "import/no-anonymous-default-export": "off",
    "no-console": "warn",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "arrowParens": "avoid",
        "trailingComma": "all"
      }
    ]
  },
  "plugins": ["prettier"]
}
